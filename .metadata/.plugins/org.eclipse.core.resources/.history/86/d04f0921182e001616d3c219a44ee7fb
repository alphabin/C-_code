#include "Date.h"
#include <iostream>

using namespace std;

Date::Date(){
	// 1 January 1970 Linux time stamp
	day=1;
	month=1;
	year=1970;

}

Date::Date(int nday,int nmonth,int nyear) {
	// TODO Auto-generated constructor stub
	day=nday;
	month=nmonth;
	year=nyear;
}


void Date::printDate(){
	  cout << day << " / " << month << " / "<<year<<endl;

};

Date::~Date() {

	// TODO Auto-generated destructor stub
}
