#include "Date.h"
#include <iostream>

using namespace std;

Date::Date(){
	// 1 January 1970 Linux time stamp
	day=1;
	month=1;
	year=1970;

}

Date::Date(int nday,int nmonth,int nyear) {
	// TODO Auto-generated constructor stub
	day=nday;
	month=nmonth;
	year=nyear;

}


Date::~Date() {
	delete day;
	delete month;
	delete year;
	// TODO Auto-generated destructor stub
}
