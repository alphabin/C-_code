void checkNum(Employee left, Programmer right ){

	   cout<<"\n ++++++ Phone ++++ ++++++ "<<endl;
	   if  (left.getPhone() == right.getPhone()){
		   cout<<endl;
		   cout <<"Equal Numbers"<<endl;
		   left.getName();
		   cout<<endl;
	   	   right.getName();
	   	   cout<<endl;
		   cout <<"Equal Numbers"<<endl;}
	   else
			cout<<"Not Equal"<<endl;

	   cout<<"\n ++++++ +++++ ++++ Number "<<endl;
	}


